
public class Car {
private String color,type;
private int year;

public Car(String c,String t, int y) {
	color = c;
	type =t;
	year =y;
}

public void display() {
	System.out.println("Color: " + color +"\nType: "+ type + "\nYear: " + year);
}
	public static void main(String[] args) {
		Car one = new Car("Blue","Honda",2009);
		one.display();

	}

}
