
public class BasicJavaTest {

	public static void main(String[] args) {
		String ans = "";
		String one = "Hacker";
		String two = "Rank";
		String[] arrOne = one.split("");
		String[] arrTwo = two.split("");
		for (int i=0;i<arrOne.length;i++) {
			if(i%2 != 0 || i == 0) {
				ans = ans + arrOne[i];
			}
		}

		System.out.println(ans);
	}

}
