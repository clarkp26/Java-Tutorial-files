
public class BasicJavaTest {

	public static void EvenOdd(String s) {
		String[] Arr = s.split("");
		String ans = "";
		String ans2 = "";
		for (int i=0;i<Arr.length;i++) {
			if(i%2 != 0) {
				ans = ans + Arr[i];
			}
			else {
				ans2 = ans2+ Arr[i];
			}
		}
		System.out.println(ans2 + " " + ans);
	}
	public static void main(String[] args) {
		
		String one = "Hacker";
		String two = "Rank";
	
		EvenOdd(one);
		EvenOdd(two);
	}

}
