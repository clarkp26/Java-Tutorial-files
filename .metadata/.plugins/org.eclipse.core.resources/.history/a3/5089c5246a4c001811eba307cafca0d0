import java.util.ArrayList;
import java.util.List;

public class ListQ {
	ArrayList<Integer> L = new ArrayList<>(400);
	
	public void insert(int x,int y) {
		L.add(y,x);
	}

	public static void main(String[] args) {
		ArrayList<Integer> L = new ArrayList<>(400);
		for(int i=0;i<L.size();i++) {
			L.add(1);
		}
		L.add(5,23);
//		for(int i=0;i<L.size();i++) {
			System.out.println(L);
//		}

	}

}
